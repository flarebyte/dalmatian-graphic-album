/** Illustrations and Panels */

table Entity {
    metadata: Metadata;
}

table Style {
    metadata: Metadata;
}

table Illustration {
    metadata: Metadata;
    data: anyUint;
}

table Shape {
    metadata: Metadata;
    drawing: [Draw];
    entityId: uint16;
    illustrationId: uint16;
}

struct Clipping {
    illustrationId: uint16;
    x: Fraction;
    y: Fraction;
    width: Fraction;
    height: Fraction;
}

union Composition { Clipping }

table Panel {
    metadata: Metadata;
    composition: [Composition];
}

struct PanelId {
    panelId: uint16;
}

struct StyleId {
    styleId: uint16;
}

struct IllustrationId {
    illustrationId: uint16;
}