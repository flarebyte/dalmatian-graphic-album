struct Section {
    supplementId: unit16;
}

struct Page {
    supplementId: unit16;
}

struct Tile {
    supplementId: unit16;
}

union TileInstruction { Section, Page, Tile, PanelId , Vec2dFraction, Dim2dFraction, StyleId, IllustrationId}

table TileLayout {
    sequence: [TileInstruction];
}

union Layout { TileLayout }

