struct Section {
    supplementId: uint16;
}

struct Page {
    supplementId: uint16;
}

struct Tile {
    supplementId: uint16;
}

union TileInstruction { Section, Page, Tile, Vec2dFraction, Dim2dFraction, StencilId, StyleId }

table TileLayout {
    sequence: [TileInstruction];
}

union Layout { TileLayout }

